.docs
  padding-left: 20px;

  h1
    margin-bottom: 40px;

  h2
    margin-bottom 20px;

  table
    margin: 20px 0;
    border-spacing: 0;
    border: 2px solid #F3F1EE;

    th
    td
      padding: 5px 10px;
      border-right: 1px solid #F3F1EE;

    td
      border-bottom: 1px solid #F3F1EE;

      &:last-child
        border-right: none;
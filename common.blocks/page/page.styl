body
html
  height: 100%;
  padding: 0px;

.page
  font: 13px Arial,Helvetica,sans-serif;
  position: relative;
  z-index: 0;
  display: flex;
  min-width: 1000px;
  margin: 0;
  padding: 0;


.page :link
  transition: all .3s ease-out;
  text-decoration: none;
  color: #01B;
  -webkit-tap-highlight-color: rgba(0,0,0,0);